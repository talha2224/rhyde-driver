{
  "expo": {
    "name": "rhyde-driver",
    "slug": "rhyde-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rhydedriver",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talhadev767.rhydedriver",
      "merchantIdentifier": "merchant.com.talhadev767.rhydedriver",
      "config": {
        "googleMapsApiKey": "AIzaSyA0P_pnW6OWas9hZhiEOt0qJ8hSopz_91s"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to show nearby drivers and provide location-based services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.talhadev767.rhydedriver",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA0P_pnW6OWas9hZhiEOt0qJ8hSopz_91s"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.talhadev767.rhydedriver"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05a72adb-b2a8-4603-85cb-2364596a9ae5"
      },
      "stripePublishableKey": "pk_live_51Rr17JQrAWYzdbHauVttOVJS4uyM2Mh4TdVHS10DeI2VPkclffHFaGUNhUJLLXV9tQmR6TccgxWfVwDvgqcjOqoY00rnjxvhUB"
    }
  }
}
